{
  "name": "roommatematching",
  "description": "Roommate Matching - template backend",
  "scripts": {
    "start": "npm run db:migrate && npm run db:seed && nodemon ./src/index.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.18.2",
    "bcrypt": "5.0.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "formidable": "1.2.2",
    "helmet": "4.1.1",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.20",
    "moment": "2.29.1",
    "multer": "^1.4.4",
    "mysql2": "2.2.5",
    "nodemailer": "6.4.14",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "passport-microsoft": "^0.1.0",
    "pg": "8.4.1",
    "pg-hstore": "2.3.3",
    "sequelize": "6.3.5",
    "sqlite": "4.0.15",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6"
  },
  "engines": {
    "node": ">=8"
  },
  "private": true,
  "devDependencies": {
    "cross-env": "7.0.2",
    "mocha": "8.1.3",
    "node-mocks-http": "1.9.0",
    "nodemon": "2.0.5",
    "sequelize-cli": "6.2.0"
  }
